<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.media.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.web.*?>

<SplitPane fx:id="root" dividerPositions="0.25, 0.5" focusTraversable="true" prefHeight="483.0" prefWidth="631.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.forgeessentials.remote.client.gui.features.PermissionsController">
  <items>
    <TreeView fx:id="zoneTree" prefHeight="287.0" prefWidth="162.0" />
    <VBox id="VBox" alignment="CENTER" spacing="5.0">
      <children>
        <Label text="Players" />
        <ListView fx:id="playerList" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
        <Label text="Groups" />
        <ListView fx:id="groupList" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      </children>
    </VBox>
    <VBox id="VBox" alignment="CENTER_LEFT" spacing="5.0">
      <children>
        <VBox id="VBox" alignment="CENTER_LEFT" spacing="5.0">
          <children>
            <Label text="Add permission" />
            <ComboBox fx:id="permissionName" editable="true" maxWidth="1.7976931348623157E308" VBox.margin="$x1">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Item 1" />
                  <String fx:value="Item 2" />
                  <String fx:value="Item 3" />
                </FXCollections>
              </items>
            </ComboBox>
            <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0">
              <children>
                <Button alignment="CENTER" mnemonicParsing="false" prefWidth="-1.0" text="Add" />
              </children>
            </HBox>
          </children>
          <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
          </padding>
        </VBox>
        <TableView fx:id="permissionTable" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="false" VBox.vgrow="ALWAYS">
          <columns>
            <TableColumn fx:id="permissionKey" text="Permission" />
            <TableColumn fx:id="permissionValue" maxWidth="2000.0" minWidth="0.0" resizable="true" text="Value" />
          </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
        </TableView>
      </children>
    </VBox>
  </items>
</SplitPane>
