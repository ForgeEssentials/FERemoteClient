<Tab fx:id="tab" text="Players" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.forgeessentials.remote.client.gui.features.PlayersController">
  <content>
    <VBox>
      <HBox fillHeight="true" prefHeight="-1.0" prefWidth="-1.0" spacing="12.0">
        <children>
          <CheckBox fx:id="flagDetails" mnemonicParsing="false" onAction="#onFlagsChanged" selected="false" text="Details" />
          <CheckBox fx:id="flagLocation" mnemonicParsing="false" onAction="#onFlagsChanged" text="Location" />
        </children>
        <VBox.margin>
          <Insets bottom="4.0" left="6.0" right="6.0" top="4.0" />
        </VBox.margin>
      </HBox>
      <TableView fx:id="playersTable" prefHeight="358.0" prefWidth="796.0">
        <columns>
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="149.0" text="Name" fx:id="colName" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="63.0" text="Health" fx:id="colHealth" />
          <TableColumn maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="Armor" fx:id="colArmor" />
          <TableColumn maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="Hunger" fx:id="colHunger" />
          <TableColumn maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="Saturation" fx:id="colSaturation" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="364.0" text="Location" fx:id="colLocation" />
        </columns>
      </TableView>
    </VBox>
  </content>
</Tab>
